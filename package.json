{
  "name": "whatsapp-api",
  "version": "1.0.0",
  "description": "Simple WhatsApp API for sending messages",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:verbose": "DEBUG=* nodemon server.js",
    "seed": "node scripts/setup-database-sql.js",
    "setup": "npm install && npm run seed",
    "setup:sql": "node scripts/setup-database-sql.js",
    "reset": "npm run setup:sql",
    "logs": "npm run dev"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.1",
    "mysql2": "^3.6.0",
    "puppeteer": "^21.0.0",
    "qrcode": "^1.5.3",
    "sequelize": "^6.32.1",
    "whatsapp-web.js": "^1.21.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
